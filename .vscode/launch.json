{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'rust_fireworks_v2'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=rust_fireworks_v2",
                    "--package=rust_fireworks_v2"
